#rundir is the directory MinKNOW is saving the fastq5s/fastqs in 
rundir = /var/lib/minknow/data/yourexperiment/sampleID/flowcell/
####################################
#the directory you are running the analysis in (not the output) 
processdir = /home/user/analysis/iSCORED/
####################################
#This have to be changed to reflect your sample
patientsample = patient_sample
outputdirname = output_iSCORED
threads = 20
basecallmodel = dna_r9.4.1_e8_sup@v3.3
### here are the possible basecalling models:
### models = dna_r9.4.1_e8_sup@v3.3
### models = dna_r10.4.1_e8.2_400bps_sup@v3.5.2
####################################
#these are paths to files/tools or code on your machine:

hg19ref = /home/user/genomes/hg19/hg19.fa
hg38ref = /home/user/genomes/hg38/hg38.fa

dorado = /home/user/dorado/bin/dorado
modeldir = /home/user/tools/dorado/models/
duplex_tools = /home/user/tools/duplex_tools/venv/bin/activate
picard = /home/user/tools/picard/build/libs/picard.jar
mbtools = /home/user/tools/mbtools/target/debug/mbtools
bwa = /home/user/tools/bwa/bwa
samtools = /home/user/tools/samtools

#path to conda environment tools 
nanofilt = /home/user/anaconda3/envs/nanofilt/bin/NanoFilt
porechop = /home/user/anaconda3/bin/porechop

#path to this repo for additional scripts 
modifiedScripts = /home/user/iSCORED/scripts/